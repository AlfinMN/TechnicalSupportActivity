<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/homeScreen">
    <action
        android:id="@+id/action_homeScreen_to_profilScreen"
        app:destination="@id/profilScreen" />
    <action
        android:id="@+id/action_homeScreen_to_mainActivity2"
        app:destination="@id/mainActivity" />
    <action
        android:id="@+id/action_global_to_homeScreen"
        app:destination="@id/homeScreen" />
    <fragment
        android:id="@+id/homeScreen"
        android:name="com.projectassyifa.technicalsupportactivities.screen.home.HomeScreen"
        android:label="fragment_home_screen"
        tools:layout="@layout/fragment_home_screen" >



    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.projectassyifa.technicalsupportactivities.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/profilScreen"
        android:name="com.projectassyifa.technicalsupportactivities.screen.ProfilScreen"
        android:label="fragment_profil_screen"
        tools:layout="@layout/fragment_profil_screen" >

    </fragment>
</navigation>